{
  "name": "@sunderapps/util",
  "version": "0.0.2",
  "description": "Sunder Apps Utility Functions",
  "main": "./src/main.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:dev && npm run build:prod && npm run build:tsdoc && npm run build:sassdoc",
    "build:dev": "webpack --config webpack.dev.ts",
    "build:prod": "webpack",
    "build:tsdoc": "typedoc",
    "build:sassdoc": "sassdoc ./src/scss -p > ./docs/sassdoc.json",
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --config webpack.dev.ts",
    "watch:prod": "webpack --watch",
    "watch:tsdoc": "typedoc --watch",
    "serve": "webpack serve --config webpack.dev.ts",
    "test": "karma start test/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunder-Apps/util.git"
  },
  "keywords": [
    "utility",
    "functions",
    "variables",
    "mixins"
  ],
  "author": "Samuel T Underwood",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sunder-Apps/util/issues"
  },
  "homepage": "https://github.com/Sunder-Apps/util#readme",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@types/node": "^17.0.42",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "jasmine-core": "^4.2.0",
    "karma": "^6.3.20",
    "karma-babel-preprocessor": "^8.0.2",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript-preprocessor": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "sass": "^1.52.3",
    "sassdoc": "^2.7.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typedoc": "^0.23.2",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-lowercase-name": "^1.0.4",
    "webpack-merge": "^5.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
